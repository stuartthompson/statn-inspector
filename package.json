{
  "name": "statn-inspector",
  "displayName": "statn-inspector",
  "description": "Gain more information about files and their contents.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:statn-inspector.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "statn-inspector.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "statn-inspector",
          "title": "Statn Inspector",
          "icon": "resources/statn-inspector.svg"
        }
      ]
    },
    "views": {
      "statn-insepctor": [
        {
          "id": "file-info",
          "name": "File Info"
        },
        {
          "id": "char-count",
          "name": "Char Count"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
